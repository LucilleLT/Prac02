<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.martin.prac02">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/main"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyStyle"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".SettingsOld"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:parentActivityName=".MainActivity"
            android:label="@string/SettingsActivity"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.martin.prac02.MainActivity"
                />
        </activity>
        <activity
            android:name=".QuotationActivity"
            android:label="@string/QuotationActivity"
            android:parentActivityName=".MainActivity"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.martin.prac02.MainActivity"
                />
        </activity>
        <activity
            android:name=".FavoriteActivity"
            android:label="@string/FavActivity"
